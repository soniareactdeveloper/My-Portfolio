#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/USER/Desktop/pot/sonia/node_modules/cssesc/bin/node_modules:/mnt/c/Users/USER/Desktop/pot/sonia/node_modules/cssesc/node_modules:/mnt/c/Users/USER/Desktop/pot/sonia/node_modules:/mnt/c/Users/USER/Desktop/pot/node_modules:/mnt/c/Users/USER/Desktop/node_modules:/mnt/c/Users/USER/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/USER/Desktop/pot/sonia/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/USER/Desktop/pot/sonia/node_modules/cssesc/bin/node_modules:/mnt/c/Users/USER/Desktop/pot/sonia/node_modules/cssesc/node_modules:/mnt/c/Users/USER/Desktop/pot/sonia/node_modules:/mnt/c/Users/USER/Desktop/pot/node_modules:/mnt/c/Users/USER/Desktop/node_modules:/mnt/c/Users/USER/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/USER/Desktop/pot/sonia/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../cssesc/bin/cssesc" "$@"
else
  exec node  "$basedir/../cssesc/bin/cssesc" "$@"
fi
